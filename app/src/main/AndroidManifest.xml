<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pl.rozekm.saucemanager">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logosvg"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logosvg"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".frontend.activities.TransactionCRUDActivity"></activity>
        <activity android:name=".frontend.activities.AllTransactionsActivity" />
        <activity android:name=".frontend.activities.StatisticsActivity" />
        <activity android:name=".frontend.activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>